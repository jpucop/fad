{
  "name": "frontend",
  "version": "1.0.0",
  "author": "jkirton@ucop.edu",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean:node": "rimraf node_modules package-lock.json",
    "clean:dist": "rimraf dist",
    "copy:static": "node ./scripts/copy-static.js",
    "build:css": "node ./scripts/build-css.js",
    "build:icons": "node ./scripts/build-icons.js",
    "inject:icons": "node ./scripts/inject-icons.js",
    "build:js": "node ./scripts/build-js.js",
    "build": "npm run copy:static && npm run build:css && npm run build:icons && npm run inject:icons && npm run build:js",
    "bld": "npm run copy:static && npm run build:css && npm run build:js",
    "dev": "npx serve ./dist"
  },
  "devDependencies": {
    "@iconify-json/mdi": "1.2.3",
    "@iconify/tools": "4.1.2",
    "@iconify/utils": "2.3.0",
    "@tailwindcss/cli": "4.1.6",
    "alpinejs": "3.14.9",
    "autoprefixer": "10.4.21",
    "esbuild": "^0.25.4",
    "postcss": "8.5.3",
    "rimraf": "6.0.1",
    "svgo": "3.3.2",
    "tailwindcss": "4.1.6",
    "serve": "14.2.4"
  }
}
