  },
  "devDependencies": {
    "@iconify/json": "^2.2.336",
    "@iconify/tailwind4": "^1.0.6",
    "iconify-cli": "git+https://github.com/iconify/cli.git",
    "tailwindcss": "^4.1.5"
  }
}
