{
  "name": "frontend",
  "version": "1.0.0",
  "author": "jkirton@ucop.edu",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "rimraf node_modules package-lock.json",
    "copy:static": "node ./scripts/copy-static.js",
    "build:css": "npx tailwindcss -i ./css/styles.css -o ./dist/css/styles.min.css --minify",
    "build:icons": "node ./scripts/build-icons.js",
    "build:js": "node scripts/build-alpine.js",
    "build": "npm run copy:static && npm run build:css && npm run build:icons && npm run build:js"
  },
  "devDependencies": {
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify/json": "^2.2.336",
    "@iconify/tailwind4": "^1.0.6",
    "@iconify/utils": "^2.3.0",
    "@tailwindcss/cli": "^4.1.5",
    "alpinejs": "^3.14.9",
    "cpx": "^1.5.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^4.1.5"
  }
}
